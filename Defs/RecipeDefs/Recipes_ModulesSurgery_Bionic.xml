			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>ComponentSpacer</li>
				<li>BionicArm</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Arm</li>
		</appliedOnFixedBodyParts>
		<addsHediff>BionicArmModule</addsHediff>
	</RecipeDef>

