<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<HediffDef ParentName="MSE_NaturalBodyPartNormal">
		<defName>Heart</defName>
		<label>heart transplant rejection</label>
		<stages>
			<li>
				<minSeverity>0.001</minSeverity>
				<label>mild rejection</label>
				<partEfficiencyOffset>-0.05</partEfficiencyOffset>
			</li>
			<li>
				<minSeverity>0.3</minSeverity>
				<label>mild rejection</label>
				<partEfficiencyOffset>-0.10</partEfficiencyOffset>
				<hediffGivers>
					<li Class="HediffGiver_Random">
						<hediff>Flu</hediff>
						<mtbDays>4</mtbDays>
					</li>
				</hediffGivers>
			</li>
			<li>
				<minSeverity>0.6</minSeverity>
				<label>acute rejection</label>
				<painOffset>0.15</painOffset>
				<partEfficiencyOffset>-0.25</partEfficiencyOffset>
				<vomitMtbDays>2</vomitMtbDays>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.2</offset>
					</li>
				</capMods>
				<hediffGivers>
					<li Class="HediffGiver_Random">
						<hediff>Flu</hediff>
						<mtbDays>2</mtbDays>
					</li>
				</hediffGivers>
			</li>
			<li>
				<minSeverity>0.8</minSeverity>
				<label>chronic rejection</label>
				<painOffset>0.30</painOffset>
				<partEfficiencyOffset>-0.60</partEfficiencyOffset>
				<vomitMtbDays>1</vomitMtbDays>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.4</offset>
					</li>
				</capMods>
				<hediffGivers>
					<li Class="HediffGiver_Random">
						<hediff>Flu</hediff>
						<mtbDays>2</mtbDays>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>HeartAttack</hediff>
						<mtbDays>2</mtbDays>
						<partsToAffect>
							<li>Heart</li>
						</partsToAffect>
					</li>
				</hediffGivers>
			</li>
			<li>
				<minSeverity>1</minSeverity>
				<label>chronic rejection</label>
				<lifeThreatening>true</lifeThreatening>
				<partEfficiencyOffset>-1.0</partEfficiencyOffset>
				<destroyPart>true</destroyPart>
			</li>
		</stages>
	</HediffDef>

